{
    "name": "paglliac/dependency-analysis",
    "description": "Library for static analysis modules dependencies in php projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kir Apukhtin",
            "email": "paglliac@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DependencyAnalysis\\": "src/",
            "DependencyAnalysis\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "nikic/php-parser": "^4.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "bin": [
        "bin/php-da"
    ],
    "scripts": {
        "test": [
            "vendor/phpunit/phpunit/phpunit --no-configuration ./tests"
        ]
    }
}
